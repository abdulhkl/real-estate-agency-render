{
  "name": "real-estate-agency",
  "version": "1.0.0",
  "description": "Real Estate Agency App",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "build": "npm i && cd frontend && npm i && npm run build"
  },
  "engines": {
    "node": "16.13.1"
  },
  "author": "Abdul Haseeb",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1202.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.6",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.9.0",
    "react-geocode": "^0.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.7"
  }
}